{
  "name": "sidelifter",
  "version": "0.0.0",
  "description": "Utilities for working with Docker containers",
  "keywords": [],
  "homepage": "https://github.com/vinsonchuong/sidelifter",
  "bugs": "https://github.com/vinsonchuong/sidelifter/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/sidelifter",
  "scripts": {
    "test": "xo && ava",
    "release": "semantic-release"
  },
  "type": "module",
  "dependencies": {
    "dockerode": "^3.2.1",
    "stream-buffers": "^3.0.2"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "rumor-mill": "^3.2.3",
    "semantic-release": "^19.0.3",
    "xo": "^0.34.1"
  },
  "ava": {
    "verbose": true,
    "timeout": "10m"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "semicolon": false,
    "rules": {
      "import/extensions": "off",
      "import/no-anonymous-default-export": "off",
      "import/no-useless-path-segments": "off",
      "unicorn/import-index": "off"
    }
  }
}
